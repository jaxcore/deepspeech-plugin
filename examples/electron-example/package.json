{
  "name": "deepspeech-electron-example",
  "productName": "deepspeech-electron-example",
  "version": "1.0.0",
  "description": "DeepSpeech running in Electron",
  "main": "public/index.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\""
  },
  "keywords": [],
  "author": {
    "name": "Dan Steinman",
    "email": "dansteinman@gmail.com"
  },
  "license": "MIT",
